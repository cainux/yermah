{
  "name": "Yermah",
  "version": "0.1",
  "description": "Very basic Yammer notifier",
  "browser_action": {
    "default_icon": "img/yermah-19.png",
    "popup": "popup.html"
  },
  "background_page": "background.html",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "icons": {
    "19": "img/yermah-19.png",
    "32": "img/yermah-32.png",
    "48": "img/yermah-48.png"
  }
}